        tools:layout_editor_absoluteX="57dp"
        tools:layout_editor_absoluteY="359dp" />

    <Button
        android:id="@+id/spielabbrechen"
        style="@style/MyButton"
        android:layout_width="270dp"
        android:layout_height="100dp"
